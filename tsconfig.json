{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "target": "es2018",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "/*": [
        "public/*"
      ]
    },
    "incremental": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": false,
    "allowUnreachableCode": true,
    "noErrorTruncation": true,
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "strictPropertyInitialization": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "forceConsistentCasingInFileNames": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "tests/**/*",
    "scripts/**/*",
    "node_modules/flowbite-react/lib/esm/index.d.ts",
    "global.d.ts",
    "cartesia.d.ts"
, "components/FileManager/FilePreview/pdf-preview/PDFPreview.tsx", "features/rich-text-editor/utils/PatternUtilsOLD.ts"  ],
  "exclude": [
    "node_modules",
    "_armani",
    "_dev"
  ],
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ],
    "transpileOnly": true,
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "./*"
        ]
      }
    }
  }
}