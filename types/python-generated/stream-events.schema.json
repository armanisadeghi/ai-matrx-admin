{
  "ChunkPayload": {
    "additionalProperties": false,
    "properties": {
      "text": {
        "title": "Text",
        "type": "string"
      }
    },
    "required": [
      "text"
    ],
    "title": "ChunkPayload",
    "type": "object"
  },
  "StatusUpdatePayload": {
    "additionalProperties": false,
    "properties": {
      "status": {
        "title": "Status",
        "type": "string"
      },
      "system_message": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "System Message"
      },
      "user_message": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "User Message"
      },
      "metadata": {
        "anyOf": [
          {
            "additionalProperties": true,
            "type": "object"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Metadata"
      }
    },
    "required": [
      "status"
    ],
    "title": "StatusUpdatePayload",
    "type": "object"
  },
  "DataPayload": {
    "additionalProperties": true,
    "properties": {},
    "title": "DataPayload",
    "type": "object"
  },
  "CompletionPayload": {
    "additionalProperties": false,
    "properties": {
      "status": {
        "default": "complete",
        "enum": [
          "complete",
          "failed",
          "max_iterations_exceeded"
        ],
        "title": "Status",
        "type": "string"
      },
      "output": {
        "default": null,
        "title": "Output"
      },
      "iterations": {
        "anyOf": [
          {
            "type": "integer"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Iterations"
      },
      "total_usage": {
        "anyOf": [
          {
            "additionalProperties": true,
            "type": "object"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Total Usage"
      },
      "timing_stats": {
        "anyOf": [
          {
            "additionalProperties": true,
            "type": "object"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Timing Stats"
      },
      "tool_call_stats": {
        "anyOf": [
          {
            "additionalProperties": true,
            "type": "object"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Tool Call Stats"
      },
      "finish_reason": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Finish Reason"
      },
      "metadata": {
        "anyOf": [
          {
            "additionalProperties": true,
            "type": "object"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Metadata"
      }
    },
    "title": "CompletionPayload",
    "type": "object"
  },
  "ErrorPayload": {
    "additionalProperties": false,
    "properties": {
      "error_type": {
        "title": "Error Type",
        "type": "string"
      },
      "message": {
        "title": "Message",
        "type": "string"
      },
      "user_message": {
        "default": "Sorry. An error occurred.",
        "title": "User Message",
        "type": "string"
      },
      "code": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Code"
      },
      "details": {
        "anyOf": [
          {
            "additionalProperties": true,
            "type": "object"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Details"
      }
    },
    "required": [
      "error_type",
      "message"
    ],
    "title": "ErrorPayload",
    "type": "object"
  },
  "ToolEventPayload": {
    "additionalProperties": false,
    "properties": {
      "event": {
        "enum": [
          "tool_started",
          "tool_progress",
          "tool_step",
          "tool_result_preview",
          "tool_completed",
          "tool_error"
        ],
        "title": "Event",
        "type": "string"
      },
      "call_id": {
        "title": "Call Id",
        "type": "string"
      },
      "tool_name": {
        "title": "Tool Name",
        "type": "string"
      },
      "timestamp": {
        "title": "Timestamp",
        "type": "number"
      },
      "message": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Message"
      },
      "show_spinner": {
        "default": true,
        "title": "Show Spinner",
        "type": "boolean"
      },
      "data": {
        "additionalProperties": true,
        "title": "Data",
        "type": "object"
      }
    },
    "required": [
      "event",
      "call_id",
      "tool_name"
    ],
    "title": "ToolEventPayload",
    "type": "object"
  },
  "BrokerPayload": {
    "additionalProperties": false,
    "properties": {
      "broker_id": {
        "title": "Broker Id",
        "type": "string"
      },
      "value": {
        "title": "Value"
      },
      "source": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Source"
      },
      "source_id": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "title": "Source Id"
      }
    },
    "required": [
      "broker_id",
      "value"
    ],
    "title": "BrokerPayload",
    "type": "object"
  },
  "HeartbeatPayload": {
    "additionalProperties": false,
    "properties": {
      "timestamp": {
        "title": "Timestamp",
        "type": "number"
      }
    },
    "title": "HeartbeatPayload",
    "type": "object"
  },
  "EndPayload": {
    "additionalProperties": false,
    "properties": {
      "reason": {
        "default": "complete",
        "title": "Reason",
        "type": "string"
      }
    },
    "title": "EndPayload",
    "type": "object"
  },
  "StreamEvent": {
    "$defs": {
      "EventType": {
        "enum": [
          "chunk",
          "status_update",
          "data",
          "completion",
          "error",
          "tool_event",
          "broker",
          "heartbeat",
          "end"
        ],
        "title": "EventType",
        "type": "string"
      }
    },
    "additionalProperties": false,
    "properties": {
      "event": {
        "$ref": "#/$defs/EventType"
      },
      "data": {
        "additionalProperties": true,
        "title": "Data",
        "type": "object"
      }
    },
    "required": [
      "event",
      "data"
    ],
    "title": "StreamEvent",
    "type": "object"
  },
  "EventType": {
    "enum": [
      "chunk",
      "status_update",
      "data",
      "completion",
      "error",
      "tool_event",
      "broker",
      "heartbeat",
      "end"
    ]
  }
}
